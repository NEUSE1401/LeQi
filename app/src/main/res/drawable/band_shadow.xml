<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp">
        <shape android:shape="oval" >
            <size android:width="85dp" android:height="85dp"/>
            <gradient
                android:angle="270"
                android:endColor="#0F000000"
                android:startColor="#0F000000" />

        </shape>
    </item>

    <item
        android:left="3dp"
        android:top="3dp"
        android:right="3dp"
        android:bottom="5dp">
        <shape android:shape="oval" >
            <size android:width="85dp" android:height="85dp"/>
            <gradient
                android:angle="270"
                android:endColor="#FFFFFF"
                android:startColor="#FFFFFF" />
        </shape>
    </item>
</layer-list>